# Backend Environment Variables
# این فایل در دایرکتوری backend استفاده می‌شود

# Port سرور backend
PORT=8000

# URL سرویس NanoBanana (API واقعی)
NANOBANANA_API_URL=https://api.nanobanana.example.com

# API Key شما از سرویس NanoBanana
# این مقدار را با API key واقعی جایگزین کنید
NANOBANANA_API_KEY=your_actual_api_key_here

# URL فرانت‌اند (برای CORS)
FRONTEND_URL=http://localhost
